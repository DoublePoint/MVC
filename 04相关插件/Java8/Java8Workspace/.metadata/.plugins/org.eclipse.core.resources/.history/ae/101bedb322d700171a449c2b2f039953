package Chap5;

import java.util.Arrays;
import java.util.List;

import org.junit.Test;

public class MainTest {
	
	@Test
	public void test83(){
		List<Integer> numbers = Arrays.asList(1, 2, 1, 3, 3, 2, 4);
		numbers.stream()
		.filter(i -> i % 2 == 0)
		.distinct()
		.forEach(System.out::println);
	}
}
