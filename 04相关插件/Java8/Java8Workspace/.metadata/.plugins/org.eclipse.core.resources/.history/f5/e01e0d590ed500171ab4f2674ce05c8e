package Test;

import java.util.ArrayList;
import java.util.List;
import java.util.function.Predicate;

public class TemplateFilter<T> {
	public static List<T> fil(List<User> list, Predicate<User> p) {

		List<User> resultList = new ArrayList<User>();
		for (User user : list) {
			if (p.test(user)) {
				resultList.add(user);
			}
		}
		return resultList;
	}
}
