package Chap1;

public class Apple {
	private long weight;
	private String color;

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public long getWeight() {
		return weight;
	}

	public void setWeight(long weight) {
		this.weight = weight;
	}

	public Apple(long weight, String color) {
		super();
		this.weight = weight;
		this.color = color;
	}
	
}
